{
  "name": "team-dev-server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "jasmine",
    "test:coverage": "nyc jasmine",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "db-reset": "prisma migrate reset",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/spec/**"
    ],
    "check-coverage": true,
    "statements": 0,
    "branches": 0,
    "functions": 0,
    "lines": 0
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boolean-uk/team-dev-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prettier": "@boolean-uk/boolean-prettier-config",
  "bugs": {
    "url": "https://github.com/boolean-uk/team-dev-server/issues"
  },
  "devDependencies": {
    "@boolean-uk/boolean-prettier-config": "^1.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "jasmine": "^5.4.0",
    "nodemon": "^2.0.15",
    "nyc": "^17.1.0",
    "prettier": "^2.6.2",
    "prisma": "^3.15.2"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.0",
    "yaml": "^2.3.4"
  }
}
